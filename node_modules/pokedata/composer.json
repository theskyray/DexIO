{
  "name": "metaunicorn/pokedata",
  "description": "Tool for exporting existing community Pokemon data to JSON using human-readable values.",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Javier Aguilar",
      "email": "git@mjolnic.com"
    }
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "veekun/pokedex",
        "version": "dev-master",
        "dist": {
          "url": "https://github.com/veekun/pokedex/archive/master.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/veekun/pokedex.git",
          "type": "git",
          "reference": "tree/master"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.6",
    "veekun/pokedex": "dev-master",
    "riimu/kit-phpencoder": "~2.1",
    "symfony/console": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "metaunicorn\\Pokedata\\": "src/"
    }
  },
  "bin": [
    "pokedata"
  ],
  "config": {
    "bin-dir": "bin",
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
